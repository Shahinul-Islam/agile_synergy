{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "components/ui/ui/button.jsx",
    "app/page.jsx",
    "app/layout.jsx",
    "components/ui/card.jsx",
    "components/ui/avatar.jsx",
    "components/ui/button.jsx",
    "components/ui/toast.jsx"
, "components/ui/textarea.jsx", "components/ui/input.jsx", "hooks/use-toast.js", "components/ui/accordion.jsx", "components/ui/badge.jsx", "components/ui/dropdown-menu.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}